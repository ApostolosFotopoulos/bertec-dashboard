{
  "name": "bertec_dashboard",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "scripts": {
    "dev:renderer": "webpack serve --config renderer/webpack.config.js --mode development",
    "dev:main": "electron ./main",
    "dev:win": "NODE_ENV=development npm-run-all -p dev:renderer dev:main",
    "dev:unix": "NODE_ENV=development concurrently --kill-others \"npm run dev:renderer\" \"npm run dev:main\" ",
    "build:renderer": "webpack build --config renderer/webpack.config.js --mode production",
    "build:mac": "electron-builder build --mac",
    "build:windows": "electron-builder build --windows"
  },
  "main": "./main/index.js",
  "dependencies": {
    "apexcharts": "^3.24.0",
    "core-js": "^3.8.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "sqlite3": "^5.0.2",
    "uuid": "^3.4.0",
    "vue": "^2.6.12",
    "vue-apexcharts": "^1.6.0",
    "vue-router": "^3.5.1",
    "vue-speedometer": "^1.8.0",
    "vuetify": "^2.4.3",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.12.11",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "*",
    "concurrently": "^6.0.2",
    "css-loader": "^5.0.1",
    "electron": "^11.2.1",
    "electron-builder": "^22.9.1",
    "electron-rebuild": "^2.3.5",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.0.0-beta.6",
    "npm-run-all": "^4.1.5",
    "vue-loader": "^15.9.6",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "vuetify-loader": "^1.7.0",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2"
  },
  "keywords": [],
  "license": "ISC",
  "build": {
    "appId": "bertec-dashboard",
    "productName": "bertec-dashboard",
    "files": [
      "assets/**/*",
      "data-fetching/**/*",
      "electron-app/**/*",
      "dist/**/*",
      "node_modules/**/*",
      "electron-helpers/**/*",
      "main.js"
    ],
    "directories": {
      "output": "build"
    },
    "extraMetadata": {
      "main": "main.js"
    },
    "mac": {
      "icon": "icons/icon.png"
    },
    "win": {
      "icon": "icons/icon.png"
    },
    "linux": {
      "icon": "icons/icon.png"
    }
  }
}
