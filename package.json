{
  "name": "bertec_dashboard",
  "version": "1.0.0",
  "description": "Analysis Dashboard",
  "author": "",
  "scripts": {
    "dev:renderer": "webpack serve --config renderer/webpack.dev.js --mode development",
    "dev:main": "nodemon --watch . --exec  'electron ./main'",
    "dev:win": "NODE_ENV=development npm-run-all -p dev:renderer dev:main",
    "dev:unix": "NODE_ENV=development concurrently --kill-others \"npm run dev:renderer\" \"npm run dev:main\" ",
    "prod": "NODE_ENV=production electron ./main",
    "build:renderer": "webpack build --config renderer/webpack.prod.js --mode production",
    "build:mac": "NODE_ENV=production electron-builder build --mac",
    "build:linux": "NODE_ENV=production electron-builder build --linux",
    "build:windows": "NODE_ENV=production electron-builder build --windows"
  },
  "main": "./main/index.js",
  "dependencies": {
    "apexcharts": "^3.24.0",
    "command-exists": "^1.2.9",
    "core-js": "^3.8.3",
    "csv-parse": "^4.15.4",
    "glob": "^7.1.7",
    "handlebars": "^4.7.7",
    "html-pdf-node": "^1.0.7",
    "jsonexport": "^3.2.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "sqlite3": "^5.0.2",
    "v-calendar": "^2.3.0",
    "vue": "^2.6.12",
    "vue-apexcharts": "^1.6.0",
    "vue-router": "^3.5.1",
    "vue-speedometer": "^1.8.0",
    "vuetify": "^2.4.3",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.12.11",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "*",
    "concurrently": "^6.0.2",
    "css-loader": "^5.0.1",
    "electron": "^13.1.1",
    "electron-builder": "^22.9.1",
    "electron-rebuild": "^2.3.5",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.0.0-beta.6",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "vue-loader": "^15.9.6",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "vuetify-loader": "^1.7.0",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2"
  },
  "keywords": [],
  "license": "ISC",
  "build": {
    "appId": "bertec-dashboard",
    "productName": "bertec-dashboard",
    "files": [
      "./.meta/databases/*",
      "./.meta/trials/*",
      "./assets/**/*",
      "./main/**/*",
      "./dist/**/*",
      "./node_modules/**/*",
      "./main/index.js"
    ],
    "directories": {
      "output": "build"
    },
    "extraMetadata": {
      "main": "./main/index.js"
    },
    "mac": {
      "icon": "./assets/icon.png"
    },
    "win": {
      "icon": "./assets/icon.png"
    },
    "linux": {
      "icon": "./assets/icon.png"
    }
  }
}
