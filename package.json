{
  "name": "bertec_dashboard",
  "version": "1.0.0",
  "description": "Analysis Dashboard",
  "author": "",
  "scripts": {
    "dev:renderer": "NODE_ENV=development webpack serve --config config/webpack.development.js",
    "dev:main": "nodemon --watch . --exec electron ./main",
    "force-plate-connector": "NODE_ENV=development node ./main/util/modules/ForcePlatesProcess.js",
    "dev:win": "NODE_ENV=development npm-run-all -p force-plate-connector dev:renderer dev:main",
    "dev:unix": "NODE_ENV=development concurrently --kill-others \"npm run dev:renderer\" \"npm run dev:main\" ",
    "prod": "NODE_ENV=production electron ./main",
    "build:renderer": "webpack build --config config/webpack.production.js --mode production",
    "build:mac": "NODE_ENV=production electron-builder build --mac",
    "build:linux": "NODE_ENV=production electron-builder build --linux",
    "build:windows": "NODE_ENV=production electron-builder build --windows"
  },
  "main": "./main/index.js",
  "dependencies": {
    "apexcharts": "^3.24.0",
    "chalk": "^4.1.2",
    "electron": "^13.1.1",
    "core-js": "^3.8.3",
    "csv-parse": "^4.15.4",
    "glob": "^7.1.7",
    "html-pdf-node": "^1.0.7",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "sqlite3": "^5.0.2",
    "v-calendar": "^2.3.0",
    "vue": "^2.6.12",
    "vue-apexcharts": "^1.6.0",
    "vue-router": "^3.5.1",
    "vue-speedometer": "^1.8.0",
    "vuetify": "^2.4.3",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/node": "^7.14.9",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "compression-webpack-plugin": "^6.0.4",
    "concurrently": "^6.0.2",
    "css-loader": "^5.2.4",
    "electron": "^13.1.1",
    "electron-builder": "^22.9.1",
    "electron-rebuild": "^2.3.5",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.1",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^8.0.16",
    "typescript": "^4.3.5",
    "vue-loader": "^15.9.8",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.14",
    "vuetify-loader": "^1.7.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.0.0",
    "webpack-merge": "^5.8.0"
  },
  "keywords": [],
  "license": "ISC",
  "build": {
    "appId": "bertec-dashboard",
    "productName": "bertec-dashboard",
    "files": [
      "./.meta/databases/*",
      "./.meta/trials/*",
      "./assets/**/*",
      "./main/**/*",
      "./dist/**/*",
      "./node_modules/**/*",
      "./main/index.js"
    ],
    "directories": {
      "output": "build"
    },
    "extraMetadata": {
      "main": "./main/index.js"
    },
    "mac": {
      "icon": "./assets/icon.png"
    },
    "win": {
      "icon": "./assets/icon.png"
    },
    "linux": {
      "icon": "./assets/icon.png"
    }
  }
}
