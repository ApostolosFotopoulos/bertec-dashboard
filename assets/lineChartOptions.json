{
  "dataLabels":{
    "enabled": false,
    "show":false
  },
  "grid": {
    "padding": {
      "top": 0,
      "right": 10,
      "bottom": 0,
      "left": 10
    }
  },
  "chart": {
    "toolbar":{
      "show":false
    },  
    "animations": {
      "enabled": false
    },
    "type": "line",
    "foreColor": "#fff"
  },
  "xaxis": {
    "forceNiceScale": true,
    "labels":{
      "style":{
        "colors":["#fff","#fff"]
      }
    },
    "type": "category"
  },
  "yaxis":{
    "forceNiceScale": false,
    "labels":{
      "style":{
        "colors":["#fff"]
      }
    }
  },
  "tooltip": {
    "show":false,
    "theme": "dark"
  },
  "fill":{
    "type":"solid",
    "colors":["#d32d41","#6ab187"]
  },
  "legend": {
    "show":false
  }
}